
model User {
  id         String    @id   @default(uuid())
  email String  @unique
  password String
  name  String?
  posts Post[]
  comments Comment[]
  chats Chat[]
  bookmarks Bookmark[]
  likes Like[]
  dislikes Dislike[]
  createdAt DateTime    @default(now())
  updatedAt DateTime    @updatedAt
  deletedAt DateTime?
}